<script type='text/javascript'>
window.addEventListener('resize', function(event) {
  if(window.innerWidth <= 800){
   location.reload();
  }
}, true);

    $("#AddToCartForm--product-template").submit(function(e){
      if(e.target[2].value == ""){
        e.preventDefault();
      e.currentTarget.classList.add('submitted');
      e.preventDefault();
            e.stopPropagation();
            e.stopImmediatePropagation();

            return false;
      }
    }); 
  
  var btn = document.getElementsByClassName("shopify-payment-button")[0];
  
  btn.addEventListener('click', function(e) {
    if($("select#SingleOptionSelector-0").val() == ""){
    e.preventDefault();
    e.stopPropagation();
      $("#AddToCartForm--product-template").addClass('submitted');
    }

    
      }, true)
 
</script>
