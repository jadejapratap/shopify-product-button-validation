<!-- Import jQuery and PhotoSwipe Scripts -->
  <script src="https://code.jquery.com/jquery-2.1.4.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/photoswipe/4.1.0/photoswipe.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/photoswipe/4.1.0/photoswipe-ui-default.min.js"></script>

  <script>
     function detectMob() {
    return ( ( window.innerWidth <= 800 ) );
  }
         if(detectMob() == true){
           
    'use strict';

    /* global jQuery, PhotoSwipe, PhotoSwipeUI_Default, console */

    (function($){

      // Init empty gallery array
      var container = [];

      // Loop over gallery items and push it to the array
      $('#ProductMediaGroup-product-template').find('.product-single__media-flex-wrapper').each(function(){
        var $link = $(this).find('img'),
          
            
            item = {
              src: $link.attr('src'),
              w: 375,
              h: 500,
              title: 'test'
            };
        container.push(item);
      });

      // Define click event on gallery item
      $('.my').click(function(event){

        // Prevent location change
        event.preventDefault();
        
        
 var isZoomed = true;

        // Define object and gallery options
        var $pswp = $('.pswp')[0],
            options = {
              index: $(this).parent('div').parent('div').parent('div').parent('div').index(),
              bgOpacity: 1,
              showHideOpacity: true,
              allowPanToNext: false,
              galleryUID: 0,
          
            getDoubleTapZoom: function(isMouseClick, item) {

                isZoomed = (gallery.getZoomLevel() == 1.5)? false : true;

                if (isMouseClick) {
                    return 1.5;
                } else {
                    return item.initialZoomLevel < 0.7 ? 1.5 : 3;
                }
            }
         

            };
         
           
        
        
        // Initialize PhotoSwipe
        var gallery = new PhotoSwipe($pswp, PhotoSwipeUI_Default, container, options);
        
        
       
        
      
       
        
        
         gallery.listen('imageLoadComplete', function(index, item) {
           
     
           
           
            if (item.w < 1 || item.h < 1) { // unknown size
                var img = new Image();
                img.onload = function() { // will get size after load
                    item.w = 375; // set image width
                    item.h = 500; // set image height
                    gallery.invalidateCurrItems(); // reinit Items
                    gallery.updateSize(true); // reinit Items
                }
                img.src = item.src; // let's download image
            }
        });

        gallery.listen('beforeChange', function() {
            if(isZoomed)
            {
                gallery.applyZoomPan(1.5, (gallery.viewportSize.x-gallery.currItem.w)/8, gallery.options.barsSize.top);
                gallery.zoomTo(1.5, {x:gallery.viewportSize.x/8,y:0}, 1.5);
            }
        });
        
        
        gallery.init();
        
               
        
        
      });
      

    }(jQuery));
           
         }else{
           'use strict';

    /* global jQuery, PhotoSwipe, PhotoSwipeUI_Default, console */

    (function($){

      // Init empty gallery array
      var container = [];

      // Loop over gallery items and push it to the array
      $('#ProductMediaGroup-product-template').find('.product-single__media-flex-wrapper').each(function(){
        var $link = $(this).find('img'),
          
            
            item = {
              src: $link.attr('src'),
              w: 1200,
              h: 1500,
              title: 'test'
            };
        container.push(item);
      });

      // Define click event on gallery item
      $('.my').click(function(event){

        // Prevent location change
        event.preventDefault();
        
        
 var isZoomed = true;

        // Define object and gallery options
        var $pswp = $('.pswp')[0],
            options = {
              index: $(this).parent('div').parent('div').parent('div').parent('div').index(),
              bgOpacity: 1,
              showHideOpacity: true,
              allowPanToNext: false,
              galleryUID: 0,
              zoomEl: true,
          
            getDoubleTapZoom: function(isMouseClick, item) {

                isZoomed = (gallery.getZoomLevel() == 1.5)? false : true;

                if (isMouseClick) {
                    return 1.5;
                } else {
                    return item.initialZoomLevel < 0.7 ? 1.5 : 3;
                }
            }
         

            };
         
           
        
        
        // Initialize PhotoSwipe
        var gallery = new PhotoSwipe($pswp, PhotoSwipeUI_Default, container, options);
        
        
       
        
      
       
        
  
        
        
        gallery.init();
        
               
        
        
      });
      

    }(jQuery));
         }
    
    
    
    
    
  </script>
