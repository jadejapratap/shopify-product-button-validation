{% endunless %}


<script>  
  $(".accordian_tab button.accordion").each(function(){
    $(this).click(function(){
    if ($(this).hasClass("active")) {
      $(this).removeClass("active");
      $(this).parent().removeClass("main_active");
    }else{
        $(".accordian_tab button.accordion").removeClass("active");
      	$(".accordian_tab button.accordion").parent().removeClass("main_active");
    	$(this).addClass("active");
      	$(this).parent().addClass("main_active");;
    }    
    });
  });
  
 $(document).ready(function() {
  $('.image-link').magnificPopup({type:'image'});
});
</script>

